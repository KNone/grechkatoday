{
    "name": "grechkatoday.ru",
    "type": "project",
    "description": "About grechka",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "require": {
        "silex/silex": "1.2.*",
        "doctrine/dbal": "2.5.*",
        "symfony/dom-crawler": "2.6.*",
        "symfony/css-selector": "2.6.*",
        "symfony/filesystem": "2.6.*",
        "symfony/yaml": "2.6.*",
        "igorw/config-service-provider": "1.2.*",
        "knplabs/console-service-provider": "dev-master",
        "websharks/html-compressor": "1.0",
        "phing/phing": "2.9.*",
        "symfony/process": "2.6.*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "websharks/html-compressor",
                "version": "1.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/websharks/html-compressor",
                    "reference": "000000-dev"
                },
                "autoload": {
                    "files": [
                        "html-compressor/includes/core.php"
                    ]
                }
            }
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
