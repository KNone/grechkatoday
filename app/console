#!/usr/bin/env php
<?php

$app = require_once __DIR__ . '/bootstrap.php';

use KNone\Grecha\Command\CreatePreview;
use KNone\Grecha\Command\ExchangeRateImport;
use KNone\Grecha\Command\PriceImport;
use KNone\Grecha\Command\CacheClear;

$application = $app['console'];
$application->add(new PriceImport());
$application->add(new ExchangeRateImport());
$application->add(new CacheClear());
$application->add(new CreatePreview());

$application->run();
